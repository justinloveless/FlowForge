{
	"name": "Sample Workflow",
	"initialState": "Start",
	"isEventDriven": false,
	"states": [
		{
			"name": "Start",
			"onEnterActions": [
				{
					"type": "Webhook",
					"parameters": {
						"url": "{{baseurl}}/webhook/test",
						"headers": {
							"X-API-KEY": "abc123"
						}
					}
				}
			],
			"onExitActions": [],
			"transitions": [
				{
					"condition": "true",
					"nextState": "Delay"
				}
			],
			"assignments": {
				"groups": [],
				"users": []
			}
		},
		{
			"name": "Delay",
			"isIdle": true,
			"onEnterActions": [
				{
					"type": "Timer",
					"parameters": {
						"relativeDelay": "00:00:10"
					}
				}
			],
			"onExitActions": [],
			"transitions": [
				{
					"condition": "event == \"Resume\"",
					"nextState": "Middle"
				}
			],
			"assignments": {
				"groups": ["system"],
				"users": []
			}
		},
		{
			"name": "Middle",
			"isIdle": true,
			"onEnterActions": [
				{
					"type": "Webhook",
					"parameters": {
						"url": "{{baseurl}}/webhook/test"
					}
				},
				{
					"type": "Custom",
					"parameters": {
						"url": "{{baseurl}}/webhook/test",
						"target": "Justin"
					}
				}
			],
			"onExitActions": [],
			"transitions": [
				{
					"condition": "event == \"Approved\" && UserState.state == \"Texas\"",
					"nextState": "Approved"
				},
				{
					"condition": "event == \"CheckAge\" && UserAge.age > 18",
					"nextState": "Declined"
				},
				{
					"condition": "event == \"CheckClasses\" && Trainings.classes.Count > 1",
					"nextState": "Declined"
				},
				{
					"condition": "event == \"Declined\"",
					"nextState": "Declined"
				},
				{
					"condition": "event == \"Cancel\"",
					"nextState": "End"
				}
			],
			"assignments": {
				"users": ["id01234", "user2@example.com"],
				"groups": ["role_admin"]
			}
		},
		{
			"name": "Approved",
			"onEnterActions": [
				{
					"type": "Webhook",
					"parameters": {
						"url": "{{baseurl}}/webhook/test"
					}
				}
			],
			"onExitActions": [
				{
					"type": "Custom",
					"parameters": {
						"url": "{{baseurl}}/webhook/test"
					}
				}],
			"transitions": [
				{
					"condition": "true",
					"nextState": "End"
				}
			],
			"assignments": {
				"groups": [],
				"users": []
			}
		},
		{
			"name": "Declined",
			"onEnterActions": [
				{
					"type": "Webhook",
					"parameters": {
						"url": "{{baseurl}}/webhook/test"
					}
				}
			],
			"onExitActions": [],
			"transitions": [
				{
					"condition": "true",
					"nextState": "End"
				}
			],
			"assignments": {
				"groups": [],
				"users": []
			}
		},
		{
			"name": "End",
			"onEnterActions": [
				{
					"type": "Webhook",
					"parameters": {
						"url": "{{baseurl}}/webhook/test"
					}
				}
			],
			"onExitActions": [],
			"transitions": [],
			"assignments": {
				"groups": [],
				"users": []
			}
		}
	]
}
