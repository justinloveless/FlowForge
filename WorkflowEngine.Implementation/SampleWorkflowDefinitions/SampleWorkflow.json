{
	"name": "Sample Workflow",
	"initialState": "Start",
	"states": [
		{
			"name": "Start",
			"webhook": "https://localhost:44307/webhook/test",
			"triggerWebhookOnExternalEvent": true,
			"isIdle": false,
			"assignments": null,
			"transitions": [
				{
					"condition": "true",
					"nextState": "Middle"
				}
			]
		},
		{
			"name": "Middle",
			"webhook": "https://localhost:44307/webhook/test",
			"triggerWebhookOnExternalEvent": true,
			"isIdle": true,
			"assignments": {
				"users": ["id01234", "user2@example.com"],
				"groups": ["role_admin"]
			},
			"transitions": [
				{
					"condition": "event == \"Approved\" && UserState.state == \"Texas\"",
					"nextState": "Approved"
				},
				{
					"condition": "event == \"CheckAge\" && UserAge.age > 18",
					"nextState": "Declined"
				},
				{
					"condition": "event == \"CheckClasses\" && Trainings.classes.Count > 1",
					"nextState": "Declined"
				},
				{
					"condition": "event == \"Declined\"",
					"nextState": "Declined"
				},
				{
					"condition": "event == \"Cancel\"",
					"nextState": "End"
				}
			]
		},
		{
			"name": "Approved",
			"webhook": "https://localhost:44307/webhook/test",
			"triggerWebhookOnExternalEvent": true,
			"isIdle": false,
			"assignments": null,
			"transitions": [
				{
					"condition": "true",
					"nextState": "End"
				}
			]
		},
		{
			"name": "Declined",
			"webhook": "https://localhost:44307/webhook/test",
			"triggerWebhookOnExternalEvent": true,
			"isIdle": false,
			"assignments": null,
			"transitions": [
				{
					"condition": "true",
					"nextState": "End"
				}
			]
		},
		{
			"name": "End",
			"webhook": "https://localhost:44307/webhook/test",
			"triggerWebhookOnExternalEvent": true,
			"isIdle": false,
			"assignments": null,
			"transitions": []
		}
	]
}